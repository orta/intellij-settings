<templateSet group="Objective-C">
  <template name="for" value="for (int $INDEX$ = 0; $COPY_INDEX$ &lt; $UPPER_BOUND$; $COPY_INDEX$++) {&#10;$END$&#10;}" description="Indexed for(;;) loop" toReformat="true" toShortenFQNames="true">
